<!--
Ant build file for JiBX/WS SOAP fault with stack trace example.

This build file "extends" the build file at ../../common.xml.  Run "ant -p" to view the full list of available targets.

This build.xml file has been tested with ant version 1.6.5. 
-->
<project name="JiBX/WS SOAP fault stack trace example" default="build">
  <property name="jibxws.examples.dir" value="../.."/>
  <property name="client.classname" value="org.jibx.ws.example.fault.trace.client.HelloClient"/>
  <property name="warname" value="jibx-ws-soap-fault-trace"/>
  <fileset id="binding.fileset" file="src/org/jibx/ws/example/fault/trace/common/hello-binding.xml"/>
  
  <import file="${jibxws.examples.dir}/common.xml" />
</project>
